{
  "name": "browserless-chrome",
  "version": "1.44.0",
  "description": "Chrome-as-a-service on your own hardware or in the cloud.",
  "repository": "browserless/chrome",
  "engines": {
    "node": ">=13.0.0"
  },
  "main": "build/index.js",
  "files": [
    "build/*",
    "debugger/*",
    "functions/*",
    "env.js",
    "hosts.json",
    "hints.json",
    "protocol.json",
    "version.json"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf build",
    "deploy": "DEBUG=browserless* ./scripts/deploy.js",
    "deploy-base": "DEBUG=browserless* ./scripts/deploy-base.js",
    "dev": "npm run build && env-cmd -f ./.env.dev node build/index.js",
    "meta": "./scripts/get-meta.js",
    "start": "node ./build",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "DEBUG=quiet jest --runInBand --detectOpenHandles",
    "symlink-chrome": "node ./scripts/symlink-chrome",
    "external": "rimraf debugger/devtools && node ./scripts/install-external-deps.js",
    "post-install": "npm run external && npm run symlink-chrome && npm run meta",
    "snyk-protect": "snyk protect"
  },
  "puppeteerVersions": {
    "puppeteer-1.20.0": {
      "puppeteer": "1.20.0",
      "chromeRevision": "686378"
    },
    "puppeteer-2.1.1": {
      "puppeteer": "2.1.1",
      "chromeRevision": "737027"
    },
    "puppeteer-3.3.0": {
      "puppeteer": "3.1.0",
      "chromeRevision": "782078"
    },
    "puppeteer-4.0.1": {
      "puppeteer": "4.0.1",
      "chromeRevision": "782078"
    },
    "puppeteer-5.4.1": {
      "puppeteer": "5.4.1",
      "chromeRevision": "809590"
    },
    "chrome-stable": {
      "puppeteer": "5.4.1",
      "chromeRevision": "809590"
    }
  },
  "releaseVersions": [
    "puppeteer-5.4.1",
    "puppeteer-4.0.1",
    "puppeteer-3.3.0",
    "puppeteer-2.1.1",
    "puppeteer-1.20.0",
    "chrome-stable"
  ],
  "author": "jgriffith",
  "license": "GPL-3.0",
  "dependencies": {
    "@types/archiver": "^5.1.0",
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/http-proxy": "^1.17.0",
    "@types/jest": "^26.0.0",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.123",
    "@types/multer": "^1.3.7",
    "@types/node": "^14.0.9",
    "@types/node-fetch": "^2.3.2",
    "@types/puppeteer": "^5.4.0",
    "@types/request": "^2.48.2",
    "@types/rimraf": "^3.0.0",
    "@types/shortid": "0.0.29",
    "archiver": "^5.0.0",
    "bluebird": "^3.5.5",
    "body-parser": "^1.18.2",
    "chromedriver": "^88.0.0",
    "cookie": "^0.4.0",
    "cors": "^2.8.4",
    "debug": "^4",
    "express": "^4.16.2",
    "express-prom-bundle": "^6.3.1",
    "extract-zip": "^2.0.1",
    "fs-extra": "^9.0.0",
    "get-port": "^5.0.0",
    "heapdump": "^0.3.15",
    "http-proxy": "^1.16.2",
    "jest": "^26.0.0",
    "joi": "^17.2.1",
