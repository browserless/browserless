    "systeminformation": "^5.0.5",
    "table": "^6.0.1",
    "tree-kill": "^1.2.0",
    "ts-jest": "^25.3.0",
    "tslint": "^6.0.0",
    "typescript": "^3.3.3",
    "untildify": "^4.0.0",
    "vm2": "^3.9.1"
  },
  "devDependencies": {
    "env-cmd": "^10.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "src",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".(ts)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$"
  },
  "snyk": true
}
