    "eslint-plugin-import": "^2.24.0",
    "prettier": "^2.2.1",
    "simple-git-hooks": "^2.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "src",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".(ts)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "setupFiles": [
      "<rootDir>/tests/jest/setEnvVars.js"
    ]
  },
  "snyk": true
}
