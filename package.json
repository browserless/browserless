{
  "name": "browserless-chrome",
  "version": "1.4.0",
  "description": "Chrome-as-a-service on your own hardware or in the cloud.",
  "repository": "joelgriffith/browserless",
  "main": "build/index.js",
  "files": [
    "build/main.css",
    "build/main.js"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf build",
    "ci": "npm run lint && DEBUG=browserless* npm test -- --bail --forceExit",
    "deploy": "DEBUG=browserless* ./scripts/deploy.js",
    "dev": "npm run build && source ./env.dev.sh && node build/index.js",
    "meta": "./scripts/get-meta.js",
    "start": "node ./build",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "jest --runInBand",
    "prepush": "npm run lint && npm test",
    "symlink-chrome": "node ./scripts/symlink-chrome"
  },
  "puppeteerVersions": {
    "puppeteer-1.7.0": "1.7.0",
    "puppeteer-1.8.0": "1.8.0",
    "puppeteer-1.9.0": "1.9.0",
    "puppeteer-1.10.0": "1.10.0",
    "puppeteer-1.11.0": "1.11.0",
    "puppeteer-1.12.2": "1.12.2",
    "puppeteer-1.13.0": "1.13.0",
    "chrome-stable": "1.4.0"
  },
  "chromeVersions": [
    "puppeteer-1.13.0",
    "puppeteer-1.12.2",
    "puppeteer-1.11.0",
    "puppeteer-1.10.0",
    "puppeteer-1.9.0",
    "puppeteer-1.8.0",
    "puppeteer-1.7.0",
    "chrome-stable"
  ],
  "author": "jgriffith",
  "license": "GPL-3.0",
  "dependencies": {
    "@cypress/xvfb": "^1.2.4",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.5",
    "@types/puppeteer": "^1.12.1",
    "body-parser": "^1.18.2",
    "chromedriver": "^2.41.0",
    "cookie": "^0.3.1",
    "cors": "^2.8.4",
    "debug": "^4.0.0",
    "express": "^4.16.2",
    "get-port": "^5.0.0",
    "http-proxy": "^1.16.2",
    "jest": "^24.1.0",
    "joi": "^14.1.1",
    "lighthouse": "^4.1.0",
    "lodash": "^4.17.11",
    "multer": "^1.4.1",
    "node-fetch": "^2.2.0",
    "node-pdftk": "^2.0.0",
    "puppeteer": "^1.13.0",
    "queue": "^6.0.0",
    "request": "^2.83.0",
    "rimraf": "^2.6.1",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "shortid": "^2.2.8",
    "tree-kill": "^1.2.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.10.0",
    "typescript": "^3.3.3",
    "vm2": "^3.6.10"
  },
  "devDependencies": {
    "husky": "^1.1.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "../coverage/",
    "collectCoverageFrom": [
      "**/*.ts",
      "!*.*"
    ],
    "rootDir": "src",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      ".(ts)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$"
  }
}
