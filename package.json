      ]
    },
    "puppeteer-14.4.1": {
      "puppeteer": "14.4.1",
      "chromeRevision": "991974",
      "platforms": [
        "linux/amd64",
        "linux/arm64"
      ]
    },
    "puppeteer-16.2.0": {
      "puppeteer": "16.2.0",
      "chromeRevision": "1022525",
